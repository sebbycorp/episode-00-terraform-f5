{
    "class": "AS3",
    "action": "deploy",
    "persist": true,
    "declaration": {
       "class": "ADC",
       "schemaVersion": "3.0.0",
       "id": "urn:uuid:33045210-3ab8-4636-9b2a-c98d22ab915d",
       "label": "Sample 1",
       "remark": "Simple HTTP application with RR pool",
       "as3": {
          "class": "Tenant",
          "A1": {
             "class": "Application",
             "template": "http",
             "serviceMain": {
                "class": "Service_HTTP",
                "virtualAddresses": [
                   "10.10.0.5"
                ],
                "pool": "web_pool"
             },
             "web_pool": {
                "class": "Pool",
                "monitors": [
                   "http"
                ],
                "members": [{
                   "servicePort": 80,
                   "serverAddresses": [
                      "10.20.0.5",
                      "10.20.0.6"
                   ]
                }]
             }
          }
       }
    }
 }